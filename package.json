{
  "name": "git-notes-bot",
  "version": "1.0.1",
  "description": "GitHub Action that posts git notes as PR comments",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "start": "node dist/index.js",
    "dashboard": "ts-node generate-dashboard.ts",
    "dashboard:watch": "ts-node generate-dashboard.ts --since '30 days ago'"
  },
  "keywords": [
    "github-action",
    "git-notes"
  ],
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vercel/ncc": "^0.38.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  }
}
